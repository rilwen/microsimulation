Import('env')
Import('core')
Import('microsim_uk')
Import('testing')
Import('gtest')
Import('GTEST_LIBS')
Import('OTHER_LIBS')
penv = env.Clone()
sources = Glob('*.cpp')
penv.Append(LIBS= [OTHER_LIBS] + GTEST_LIBS + [testing])
microsim_uk_tests = penv.Program('microsim-uk-tests', sources + microsim_uk + core + gtest)
Return('microsim_uk_tests')

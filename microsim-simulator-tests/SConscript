Import('env')
Import('core')
Import('microsim_simulator')
Import('microsim_core')
Import('microsim_calibrator')
Import('testing')
Import('gtest')
Import('GTEST_LIBS')
Import('OTHER_LIBS')
penv = env.Clone()
#penv.Append(CPPPATH=['#/microsim-simulator'])
sources = Glob('*.cpp')
penv.Append(LIBS= [OTHER_LIBS] + GTEST_LIBS + [testing])
microsim_simulator_tests = penv.Program('microsim-simulator-tests', sources + microsim_simulator + microsim_core + microsim_calibrator + core + gtest)
Return('microsim_simulator_tests')

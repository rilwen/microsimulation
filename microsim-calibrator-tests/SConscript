Import('env')
Import('core')
Import('microsim_calibrator')
Import('microsim_core')
Import('microsim_simulator')
Import('testing')
Import('gtest')
Import('GTEST_LIBS')
Import('OTHER_LIBS')
penv = env.Clone()
#penv.Append(CPPPATH=['#/microsim-calibrator'])
sources = Glob('*.cpp')
penv.Append(LIBS= [OTHER_LIBS] + GTEST_LIBS + [testing])
microsim_calibrator_tests = penv.Program('microsim-calibrator-tests', sources + microsim_calibrator + microsim_simulator + microsim_core + core + gtest)
Return('microsim_calibrator_tests')

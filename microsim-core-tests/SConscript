Import('env')
Import('core')
Import('gtest')
Import('microsim_core')
#Import('testing')
Import('GTEST_LIBS')
Import('OTHER_LIBS')
penv = env.Clone()
#penv.Append(CPPPATH=['#/microsim-core'])
sources = Glob('*.cpp')
penv.Append(LIBS= [OTHER_LIBS] + GTEST_LIBS)
microsim_core_tests = penv.Program('microsim-core-tests', sources + microsim_core + core + gtest)
Return('microsim_core_tests')
